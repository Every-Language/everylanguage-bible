{
  "expo": {
    "name": "el-bible",
    "slug": "el-bible",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.everylanguage.elbible"
    },
    "android": {
      "package": "com.everylanguage.elbible"
    },
    "plugins": [
      "expo-sqlite",
      "expo-router",
      "expo-font",
      "expo-localization",
      "expo-audio",
      "expo-notifications"
    ],
    "scheme": "el-bible",
    "newArchEnabled": true
  }
}
